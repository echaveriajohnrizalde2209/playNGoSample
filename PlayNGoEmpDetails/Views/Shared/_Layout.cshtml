<!DOCTYPE html>
<html lang="en">
    <head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title - My ASP.NET MVC Application</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
        <meta name="viewport" content="width=device-width" />
                @Styles.Render("~/Content/bootstrap.css")
                @Styles.Render("~/Content/site.css")
              @Scripts.Render("~/bundles/modernizr")
    </head>
    <body>
        <div class = "container">
      	<div class = "row" style="background-color: #dedef8;">
            <div class="col-sm-12 col-md-12 col-lg-12 col-sm-offset-5 "> 
                <h2>EMPLOYEE VIEW</h2>
                <br />
                <br />
            </div>
        </div>
            <br />
            <br />
        <div class="row">
            <ul class="nav nav-tabs">
                <li class="active"><a href="#empDetails" data-toggle="tab">Employee Lists</a></li>
            </ul>
        </div>
        <div class="row">
            	<div id="myTabContent" class="tab-content">    
		            <div class="tab-pane fade in active" id="empDetails">       
                         @RenderBody()
                    </div>
                </div>
        </div>
        <br />
        <br />
   
            	<div class = "row" style="background-color: #dedef8;">
                         <br />
                       <div class="col-sm-12 col-md-12 col-lg-12 col-sm-offset-5 "> 
                     <p>&copy; @DateTime.Now.Year - My ASP.NET MVC Application</p>       
                <br />
                <br />
            </div>
            </div>
      
        </div>
    </body>
</html>

  @Scripts.Render("~/bundles/jquery")
        @RenderSection("scripts", required: false)
















